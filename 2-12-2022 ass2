def vote(age):
    if age>=18:
        return 'Yes'
    else:
        return 'No'
age=int(input('Enter age:'))
print(vote(age))


def vote(age):
    if age>=18:
        return True
    else:
        return False
age=int(input('Enter age:'))
if(vote(age)):
    print('Yes')
else:
    print('No')
