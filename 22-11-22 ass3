sno=int(input('enter number:'))
name=input('enter name:')
gender=input('enter gender:')
group=input('enter group:')
phnumber=int(input('enter phnumber:'))
address=input('enter address:')
degreepercentage=float(input('enter degreepercentage:'))
print('sno',sno)
print('name',name)
print('gender',gender)
print('group',group)
print('phnumber',phnumber)
print('address',address)
print('degreepercentage',degreepercentage)
